{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "type": "object",
    "properties": {
      "base_url": {
        "type": "string",
        "format": "uri",
        "description": "The base URL for fetching data."
      },
      "fetch_rate": {
        "type": "integer",
        "minimum": 1,
        "description": "The rate at which data should be fetched (in seconds)."
      }
    },
    "required": ["base_url", "fetch_rate"],
    "additionalProperties": false
  }